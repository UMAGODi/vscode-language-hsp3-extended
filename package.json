{
    "name": "language-hsp3-ex",
    "displayName": "language-hsp3-ex",
    "description": "extended syntax highlighting for language-hsp3",
    "version": "1.0.1",
    "repository": "https://github.com/UMAGODi/vscode-language-hsp3-extended",
    "publisher": "UMAGODi",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "hsp3",
            "aliases": ["HSP3", "hsp3"],
            "extensions": [".hsp",".as"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "hsp3",
            "scopeName": "source.hsp3-ex",
            "path": "./syntaxes/hsp3.tmLanguage.json"
        }]
    }
}